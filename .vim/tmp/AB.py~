import math
def createString(N, K):
    bCount = int(math.ceil(N/2.0))
    aCount = int(N/2)
    if K > aCount * bCount:
        return ""
    result = ['A']*aCount + ['B']*bCount

    value = aCount * bCount
    firstB = aCount
    while value > K:
        swap = (value % bCount)
        if not swap == 0:
            swap = bCount - swap
        result[firstB+swap] = 'A'
        result[firstB+swap-1] = 'B'
        value -= 1
        if value % bCount == 0:
            firstB -= 1

    return ''.join(result)

print createString(5, 8)
print createString(5, 5)
print createString(10, 12)
